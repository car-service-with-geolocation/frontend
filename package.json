{
  "name": "car-service-with-geolocation-frontend",
  "version": "0.1.0",
  "private": true,
  "homepage": "./",
  "scripts": {
    "dev": "set REACT_APP_ENV=development&& react-scripts start",
    "start": "set REACT_APP_ENV=production&& react-scripts start",
    "build": "react-scripts build",
    "lint": "eslint src --ext .js,.jsx,.ts,.tsx",
    "lint:fix": "eslint src --ext .js,.jsx,.ts,.tsx --fix",
    "format": "prettier --write \"src/**/*.{js,jsx,ts,tsx,css,scss,md,json}\" --ignore-path ./.prettierignore",
    "sb:dev": "storybook dev -p 6006",
    "sb:build": "storybook build",
    "sb:deploy": "gh-pages -d storybook-static",
    "test": "react-scripts test",
    "prepare": "husky install",
    "commit": "git-cz",
    "commitlint": "commitlint --edit",
    "preinstall": "npx --yes force-resolutions"
  },
  "type": "module",
  "browserslist": {
    "production": [
      ">0.2%",
      "not dead",
      "not op_mini all"
    ],
    "development": [
      "last 1 chrome version",
      "last 1 firefox version",
      "last 1 safari version"
    ]
  },
  "dependencies": {
    "@testing-library/jest-dom": "^5.17.0",
    "@testing-library/react": "^13.4.0",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.5.2",
    "@types/node": "^16.18.46",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-redux": "^8.1.2",
    "react-router-dom": "^6.15.0",
    "react-scripts": "^5.0.1",
    "react-select": "^5.7.4",
    "redux": "^4.2.1",
    "typescript": "^4.9.5",
    "web-vitals": "^2.1.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^17.7.1",
    "@commitlint/config-conventional": "^17.7.0",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/addon-onboarding": "^1.0.8",
    "@storybook/blocks": "^7.4.0",
    "@storybook/preset-create-react-app": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/react-webpack5": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@typescript-eslint/eslint-plugin": "^6.5.0",
    "@typescript-eslint/parser": "^6.5.0",
    "babel-plugin-named-exports-order": "^0.0.2",
    "commitizen": "^4.3.0",
    "cz-git": "^1.7.1",
    "eslint": "^8.48.0",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-airbnb-typescript": "^17.1.0",
    "eslint-config-prettier": "^9.0.0",
    "eslint-plugin-import": "^2.28.1",
    "eslint-plugin-jsx-a11y": "^6.7.1",
    "eslint-plugin-prettier": "^5.0.0",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-react-hooks": "^4.6.0",
    "eslint-plugin-react-refresh": "^0.4.3",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "gh-pages": "^6.0.0",
    "husky": "^8.0.3",
    "lint-staged": "^14.0.1",
    "prettier": "3.0.3",
    "prop-types": "^15.8.1",
    "storybook": "^7.4.0",
    "webpack": "^5.88.2"
  },
  "resolutions": {
    "@svgr/webpack": "^8.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "workbox-background-sync": "^6.6.0",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  },
  "overrides": {
    "@svgr/webpack": "^8.1.0",
    "@rollup/plugin-terser": "^0.4.3",
    "workbox-background-sync": "^6.6.0",
    "@jridgewell/sourcemap-codec": "^1.4.15",
    "@babel/plugin-proposal-private-property-in-object": "^7.21.11"
  },
  "husky": {
    "hooks": {
      "commit-msg": "commitlint -E HUSKY_GIT_PARAMS -g './commitlint.config.cjs'"
    }
  },
  "lint-staged": {
    "*.{ts,tsx,js,jsx}": "npm run lint",
    "*.{ts,tsx,js,jsx,css,md,html,json}": "npm run format"
  },
  "config": {
    "commitizen": {
      "path": "node_modules/cz-git",
      "useEmoji": false,
      "czConfig": "cz.config.js"
    }
  },
  "eslintConfig": {
    "extends": [
      "plugin:storybook/recommended"
    ]
  }
}
